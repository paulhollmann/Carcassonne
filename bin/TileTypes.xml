<?xml version="1.0"?>
<tiletypes>
	<!-- Beispielformat

	<tile type="" ammount="" pennant="">
		<node feature="" position=""/>
		<edge a="" b=""/>
	</tile>

	"type" name of tile, as named in the official rules (also it is the filename of the tiles image)
	"pennant" can be true or left out
	"feature" can be FIELDS, ROAD, CASTLE, MONASTERY
	"position" can be TOPLEFT, TOP, TOPRIGHT, LEFT, CENTER, RIGHT, BOTTOMLEFT, BOTTOM, BOTTOMRIGHT
	edge connects positions "a" and "b".

	-->

	<tile type="A" ammount="2">
		<node feature="FIELDS" position="TOP"/>
		<node feature="FIELDS" position="LEFT" meeplespot="true"/>
		<node feature="MONASTERY" position="CENTER" meeplespot="true"/>
		<node feature="FIELDS" position="RIGHT"/>
		<node feature="FIELDS" position="BOTTOMLEFT"/>
		<node feature="ROAD" position="BOTTOM" meeplespot="true"/>
		<node feature="FIELDS" position="BOTTOMRIGHT"/>
		<edge a="TOP" b="LEFT" />
		<edge a="TOP" b="RIGHT" />
		<edge a="LEFT" b="BOTTOMLEFT" />
		<edge a="RIGHT" b="BOTTOMRIGHT"/>
	</tile>

	<tile type="B" ammount="4">
